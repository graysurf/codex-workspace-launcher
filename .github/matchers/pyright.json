{
  "problemMatcher": [
    {
      "owner": "pyright",
      "pattern": [
        {
          "regexp": "^([^:]+):(\\d+):(\\d+)\\s+-\\s+(error|warning|information):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}
